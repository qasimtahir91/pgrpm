[pgdg80]
name=PostgreSQL 8.0 $releasever - $basearch
baseurl=https://yum.postgresql.org/8.0/fedora/fedora-$releasever-$basearch
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-PGDG

[pgdg80-source]
name=PostgreSQL 8.0 $releasever - $basearch - Source
failovermethod=priority
baseurl=https://yum.postgresql.org/srpms/8.0/fedora/fedora-$releasever-$basearch
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-PGDG

